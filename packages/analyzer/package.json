{
  "name": "@web-component-analyzer/analyzer",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run copy:config",
    "build:clean": "tsc --build --clean && tsc --build && npm run copy:config",
    "copy:config": "mkdir -p dist/config && cp src/config/library-adapters.json dist/config/",
    "dev": "tsc --watch",
    "lint": "eslint src"
  },
  "dependencies": {},
  "peerDependencies": {
    "@swc/core": "^1.15.1"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "license": "Apache-2.0"
}
